{
  "compileOnSave": false,
  "angularCompilerOptions": {
    "...": null,
    "disableTypeScriptVersionCheck": true,
    "enableIvy": true
  },
  "compilerOptions": {
    "baseUrl": "./",
    "module": "esnext",
    "outDir": "./dist/out-tsc",
    "sourceMap": false,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "resolveJsonModule": true,
    "esModuleInterop": true ,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2015",
      "es2017",
      "dom"
    ]
  }
}
